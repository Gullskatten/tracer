<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS}  %-6p%50.50c : %m%n</pattern>
        </encoder>
    </appender>

    <logger name="com.tyrill.tracer" level="INFO">
        <appender-ref ref="STDOUT" />
    </logger>

</configuration>